	<Field name="DefTxt" type="java.lang.String"><![CDATA[import java.sql as sql
import java.lang as lang
import sys
sys.path.append("/GAI/11_GAI/0552_GAI_FUENTES_CONCILIACION/02_BATS/InterpreteFormula.jar")
import interpreteformulas.InterpreteFormulas as InterpreteFormulas

def main():

    #### Crear un objeto InterpreteFormulas
    iform = InterpreteFormulas() 

    #### Registrar driver
    driver, url, user, passwd = (
        '<?=odiRef.getInfo("SRC_JAVA_DRIVER") ?>',
        '<?=odiRef.getInfo("SRC_JAVA_URL") ?>',
        '<?=odiRef.getInfo("SRC_USER_NAME") ?>',
        '<?=odiRef.getInfo("SRC_PASS") ?>')

    lang.Class.forName(driver)
    #### Crear un objeto Connection
    myCon = sql.DriverManager.getConnection(url, user, passwd)
    myCon.setSchema('STAGE')
    try:
        #### Crear sentencia para bucle principal
        myStmt1 = myCon.createStatement(sql.ResultSet.TYPE_FORWARD_ONLY, sql.ResultSet.CONCUR_UPDATABLE)
        #### Ejecutar consulta para bucle principal
        qry1 = "select FORMULA, VALOR from TBL_GAI_FORMULA"
        myRs1 = myStmt1.executeQuery(qry1)
        #### Crear sentencia para bucle interno
        myStmt2 = myCon.createStatement()
        #### Ejecutar consulta para bucle interno
        qry2 = "select VALOR1 from TBL_GAI_PARAMETRO where TIPO = 'FORMULA' and PARAMETRO = 'variable'"  
        ##### Recorrer el resultado
        while (myRs1.next()):
            #### Obtener valor del campo FORMULA
            formula = myRs1.getString("FORMULA")
            #### Obtener arreglo de parÃ¡metros
            variables = iform.getVariables(formula)
            raise Exception(variables)
            variables_valor = []
            for variable in variables:
                qry3 = qry2.replace("variable",variable)
                myRs2 = myStmt2.executeQuery(qry3)
                myRs2.next()
                variables_valor.append(variable+'!:'+myRs2.getString("VALOR1"))
            raise Exception(variables_valor)
            #### variables_valor = ['PARAMETROS_VAR_P1!:50.5', 'PARAMETROS_VAR_P2!:50','PARAMETROS_VAR_P3!:15','PARAMETROS_VAR_P4!:5']
            resultado = iform.calcularFormula(formula, variables_valor)
            myRs1.updateString("VALOR", str(resultado))
            myRs1.updateRow()

    finally:
        myCon.close()

### Entry Point of the program      
if __name__ == '__main__':
    main()]]></Field>
