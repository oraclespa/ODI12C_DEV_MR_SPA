def new_user(user, pwd, supervisor ) {
	 expirationDate = null
	
	OdiInstance odiInstance = odiRef.getOdiInstance();
	txnDef    = new DefaultTransactionDefinition()
	tm        = odiInstance.getTransactionManager()
	tme       = odiInstance.getTransactionalEntityManager()
	txnStatus = tm.getTransaction(txnDef)

	// chequeo si el usuario ya existe
	userf = (IOdiUserFinder) tme.getFinder(OdiUser.class)

	// creacion del usuario
	OdiUserCreationServiceImpl service = new OdiUserCreationServiceImpl(odiInstance)
	OdiUser newUser = service.createOdiUser(user, pwd.toCharArray(), supervisor.toBoolean(), expirationDate );
	tm.commit(txnStatus)
	
	return newUser
}

def adding_profiles(username,notes) {
	// codigo generico para transacciones
	OdiInstance odiInstance = odiRef.getOdiInstance();
	txnDef    = new DefaultTransactionDefinition()
	tm        = odiInstance.getTransactionManager()
	tme       = odiInstance.getTransactionalEntityManager()
	txnStatus = tm.getTransaction(txnDef)
 
         
        // se busca el usuario
        user = odiInstance.getTransactionalEntityManager().getFinder(OdiUser.class).findByName(username) ;
		user.setPasswordExpiracyDate(new Date());
        user.setNotes(notes);

        // se buscan los perfiles requeridos y se guardan en una variable
		genericprofile_connect = odiInstance.getTransactionalEntityManager().getFinder(OdiProfile.class).findByName("CONNECT") ;
        genericprofile_designer = odiInstance.getTransactionalEntityManager().getFinder(OdiProfile.class).findByName("DESIGNER") ;
        genericprofile_est_prev = odiInstance.getTransactionalEntityManager().getFinder(OdiProfile.class).findByName("P_ESTANDAR_PREV_DESARROLLO") ;
        genericprofile_est_prod = odiInstance.getTransactionalEntityManager().getFinder(OdiProfile.class).findByName("P_ESTANDAR_PROD_DESARROLLO") ;
          // se aÃ±aden los perfiles al usuario
        user.addOdiProfile(genericprofile_connect); 
        user.addOdiProfile(genericprofile_designer); 
		user.addOdiProfile(genericprofile_est_prev);
		user.addOdiProfile(genericprofile_est_prod);

	tm.commit(txnStatus)
	return user;
}

